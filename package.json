{
  "name": "storage-service-zip",
  "version": "1.0.100",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type:check": "tsc --noEmit",
    "build": "rimraf build && vite build --outDir build --ssr src/index.ts",
    "build:tsc": "rimraf build && tsc -p tsconfig.json",
    "start": "node build/index.js",
    "dev": "chcp 65001 && tsx watch src .env | pino-pretty --colorize",
    "dev:vite": "chcp 65001 && vite | pino-pretty --colorize",
    "prepare": "husky install",
    "cz": "git add . & czg"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.31.2",
    "adm-zip": "^0.5.10",
    "ajv": "^8.12.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "env-schema": "^5.2.0",
    "fastify": "^4.21.0",
    "fastify-plugin": "^4.5.1",
    "husky": "^8.0.0",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^20.5.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "czg": "^1.7.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-prettier": "^5.0.0",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-node": "^3.0.2"
  }
}
